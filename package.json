{
  "name": "capy-js",
  "version": "0.0.1",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon index.js",
    "build": "esbuild index.js --bundle --minify --platform=node --format=cjs --loader:.node=copy --outdir=dist",
    "build:adapter": "esbuild adapters/geckos/index.js --bundle --minify --platform=node --format=cjs --loader:.node=copy --outdir=dist/adapters/geckos",
    "types": "npx -p typescript tsc index.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "docs": "jsdoc src/adapters.js src/manager.js src/state.js src/ticker.js -d docs"
  },
  "keywords": [
    "multiplayer"
  ],
  "author": "FlokiTV",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@gullerya/object-observer": "^6.1.1",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "esbuild": "^0.17.14",
    "jsdoc": "^4.0.2"
  }
}
